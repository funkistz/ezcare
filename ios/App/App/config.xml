<?xml version='1.0' encoding='utf-8'?>
<widget version="1.0.0" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
  <access origin="*" />
  
  <feature name="CallNumber">
    <param name="ios-package" value="CFCallNumber"/>
  </feature>

  <feature name="PhotoViewer">
    <param name="ios-package" value="PhotoViewer"/>
  </feature>

  <feature name="Chooser">
    <param name="ios-package" value="Chooser"/>
  </feature>

  <feature name="CropPlugin">
    <param name="ios-package" value="CTCrop"/>
  </feature>

  <feature name="SitewaertsDocumentViewer">
    <param name="ios-package" value="SitewaertsDocumentViewer"/>
  </feature>

  <feature name="File">
    <param name="ios-package" value="CDVFile"/>
    <param name="onload" value="true"/>
  </feature>

  <feature name="Geolocation">
    <param name="ios-package" value="CDVLocation"/>
  </feature>

  <feature name="InAppBrowser">
    <param name="ios-package" value="CDVWKInAppBrowser"/>
    <param name="onload" value="true"/>
  </feature>

  <feature name="NativeGeocoder">
    <param name="ios-package" value="NativeGeocoder"/>
  </feature>

  <feature name="PreviewAnyFile">
    <param name="ios-package" value="HWPPreviewAnyFile"/>
  </feature>

  <feature name="LaunchNavigator">
    <param name="ios-package" value="LaunchNavigatorPlugin"/>
  </feature>

  
</widget>